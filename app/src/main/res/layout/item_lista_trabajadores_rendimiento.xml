<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:id="@+id/item_fila_persona"
        android:layout_margin="2dp"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <LinearLayout
            android:layout_weight="0"
            android:orientation="vertical"
            android:layout_width="wrap_content"
            android:layout_height="match_parent">

            <TextView
                android:visibility="gone"
                android:id="@+id/item_cab_dni"
                android:layout_marginRight="1dp"
                android:background="#2d2d2d"
                android:textColor="#fff"
                android:padding="5dp"
                android:textSize="18sp"
                android:gravity="center"
                android:text="DNI"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />

            <TextView
                android:id="@+id/item_dni"
                android:textColor="#000"
                android:paddingRight="5dp"
                android:paddingLeft="5dp"
                android:paddingBottom="10dp"
                android:paddingTop="10dp"
                android:textSize="14sp"
                android:gravity="center"
                android:text="--------"
                android:textStyle="bold"
                android:layout_width="80dp"
                android:layout_height="wrap_content" />

        </LinearLayout>

        <LinearLayout
            android:layout_weight="3"
            android:orientation="vertical"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <TextView
                android:visibility="gone"
                android:id="@+id/item_cab_nombres"
                android:layout_marginLeft="1dp"
                android:layout_marginRight="1dp"
                android:background="#2d2d2d"
                android:textColor="#fff"
                android:padding="5dp"
                android:textSize="18sp"
                android:gravity="center"
                android:text="NOMBRES"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />

            <TextView
                android:id="@+id/item_nombres"
                android:textColor="#000"
                android:paddingRight="5dp"
                android:paddingLeft="5dp"
                android:paddingBottom="10dp"
                android:paddingTop="10dp"
                android:textSize="15sp"
                android:gravity="center"
                android:textStyle="bold"
                android:text="AASIUISIAS AASJHA "
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />


        </LinearLayout>

        <LinearLayout
            android:layout_weight="0"
            android:orientation="vertical"
            android:layout_width="wrap_content"
            android:layout_height="match_parent">

            <TextView
                android:visibility="gone"
                android:id="@+id/item_cab_rend"
                android:layout_marginLeft="1dp"
                android:background="#2d2d2d"
                android:textColor="#fff"
                android:padding="5dp"
                android:textSize="18sp"
                android:gravity="center"
                android:text="REND."
                android:layout_width="80dp"
                android:layout_height="wrap_content" />

            <TextView
                android:id="@+id/item_rendimiento"
                android:textColor="#000"
                android:paddingRight="5dp"
                android:paddingLeft="5dp"
                android:paddingBottom="10dp"
                android:paddingTop="10dp"
                android:textSize="16sp"
                android:textStyle="bold"
                android:gravity="center"
                android:text="00"
                android:layout_width="80dp"
                android:layout_height="wrap_content" />

        </LinearLayout>




    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">






    </LinearLayout>

</LinearLayout>